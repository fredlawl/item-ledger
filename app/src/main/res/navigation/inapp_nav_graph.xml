    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/inapp_nav_graph"
    app:startDestination="@id/Inventory"
    android:label="In App Nav Graph">

    <fragment
        android:id="@+id/Inventory"
        android:name="com.fredlawl.itemledger.Inventory"
        android:label="Inventory"
        tools:layout="@layout/fragment_inventory">

        <action
            android:id="@+id/action_new_character"
            app:destination="@+id/NewCharacter" />

    </fragment>

    <fragment
        android:id="@+id/NewCharacter"
        android:name="com.fredlawl.itemledger.NewCharacter"
        android:label="Inventory"
        tools:layout="@layout/fragment_new_character_form">
    </fragment>

</navigation>